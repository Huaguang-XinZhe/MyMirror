{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "https://huaguang-xinzhe.github.io/MyMirror-Static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
